{
  "name": "socket-generator",
  "version": "1.0.6",
  "description": "Turn any async generator / function into a Websocket stream in 2 lines.",
  "engineStrict": false,
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "engines": {
    "node": ">= 12.0.0"
  },
  "keywords": [
    "generator",
    "async",
    "socket",
    "socket.io",
    "websocket",
    "stream",
    "contract"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gittyeric/socket-generator"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.0.0",
    "babel-jest": "^29.0.2",
    "jest": "^29.0.2",
    "random-seed": "^0.3.0",
    "rimraf": "3.0.2",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp dist",
    "build": "npm run build:ts-js && npm run build:ts-types",
    "build:ts-js": "tsc -p tsconfig.json",
    "build:ts-types": "tsc -p tsconfig.release.json",
    "deploy-dry": "npm run build && git status",
    "watch": "tsc -w -p tsconfig.release.json",
    "test": "npm run test-only",
    "test-only": "jest --coverage -i --testTimeout=10000 --detectOpenHandles"
  },
  "author": "Eric <git@letsmakeit.com>",
  "license": "GNU"
}
